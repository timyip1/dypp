<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>



<table>
  <tr>
    <th>ID</th>
    <th>Email </th>
    <th>Username</th>
  </tr>
 
<% @users = User.all %>
<% @users.each do |user| %>
    <tr>
      <td> <%= user.id %></td>
      <td> <%= user.email %></td>
      <td> <%= user.username %></td>
      <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
 


  </tr>
  <% end %>

  </table>
